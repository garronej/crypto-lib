{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es5",
            "es2015"
        ],
        "declaration": true,
        "outDir": "./dist",
        "strictNullChecks": true,
        "sourceMap": false,
        "newLine": "LF",
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": true,
        "composite": true,
        "types": []
    },
    "references": [
        {
            "path": "./builder"
        },
        {
            "path": "./sync"
        },
        {
            "path": "./async"
        }
    ]
}
